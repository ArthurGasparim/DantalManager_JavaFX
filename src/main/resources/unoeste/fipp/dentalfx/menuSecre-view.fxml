<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="254.0" prefWidth="424.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.dentalfx.MenuSecreController">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="Funcionalidades">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onAgendamento" text="Agendamento" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Tabelas">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onDentista" text="Dentistas" />
                        <MenuItem mnemonicParsing="false" onAction="#onMaterial" text="Material" />
                        <MenuItem mnemonicParsing="false" onAction="#onProcedimento" text="Procedimento" />
                        <MenuItem mnemonicParsing="false" onAction="#onPaciente" text="Paciente" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onSobre" text="Sobre..." />
                        <MenuItem mnemonicParsing="false" onAction="#onTopicos" text="Topicos..." />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
