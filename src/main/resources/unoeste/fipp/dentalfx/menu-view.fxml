<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="254.0" prefWidth="424.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.dentalfx.MenuController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Funcionalidades">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onAgendamento" text="Agendamento" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Tabelas">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onDentista" text="Dentistas" />
                  <MenuItem mnemonicParsing="false" onAction="#onMaterial" text="Material" />
                  <MenuItem mnemonicParsing="false" onAction="#onProcedimento" text="Procedimento" />
                  <MenuItem mnemonicParsing="false" onAction="#onPaciente" text="Paciente" />
                  <MenuItem mnemonicParsing="false" onAction="#onUsuario" text="Usuario" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Relatiorios">
               <items>
                  <Menu mnemonicParsing="false" text="Relatorios Basicos">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#onRelPaciente" text="Paciente" />
                        <MenuItem mnemonicParsing="false" onAction="#onRelProcedimento" text="Procedimento" />
                        <MenuItem mnemonicParsing="false" onAction="#onRelMaterial" text="Material" />
                        <MenuItem mnemonicParsing="false" onAction="#onRelDentista" text="Dentista" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Relatorios Analiticos">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#onRelAtendimento" text="Atendimento" />
                     </items>
                  </Menu>
               </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
