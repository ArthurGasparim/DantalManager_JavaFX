<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="360.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.dentalfx.UsuarioFormController">
    <children>
        <VBox prefHeight="380.0" prefWidth="700.0" spacing="10.0" VBox.vgrow="ALWAYS">
            <children>
                <Label text="ID">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <TextField fx:id="tfId" editable="false" prefHeight="25.0" prefWidth="75.0" />
                <Label layoutX="10.0" layoutY="10.0" text="Nome">
                    <font>
                        <Font size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="tfNome" layoutX="10.0" layoutY="35.0" prefHeight="25.0" prefWidth="75.0" />
                <Label layoutX="10.0" layoutY="61.0" text="Nivel">
                    <font>
                        <Font size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="tfNovel" layoutX="10.0" layoutY="86.0" prefHeight="25.0" prefWidth="75.0" />
            <Label layoutX="20.0" layoutY="181.0" text="Senha">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="tfSenha" layoutX="20.0" layoutY="217.0" prefHeight="25.0" prefWidth="75.0" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
        <HBox alignment="CENTER" prefHeight="143.0" prefWidth="700.0" spacing="40.0" style="-fx-background-color: black;">
            <children>
                <Button mnemonicParsing="false" onAction="#onConfirmar" text="Confirmar" />
                <Button mnemonicParsing="false" onAction="#onCancelar" text="Cancelar" />
            </children>
        </HBox>
    </children>
</VBox>
